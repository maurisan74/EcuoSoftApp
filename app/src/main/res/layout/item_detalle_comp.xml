<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
            xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/colorPrimary"
            app:cardCornerRadius="6dp"
            app:cardElevation="4dp"
            app:contentPadding="5dp">


    <androidx.cardview.widget.CardView
        android:id="@+id/cvItem1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="5dp"
        android:background="@color/colorAccent"
        app:cardBackgroundColor="@color/LightGray"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginStart="3dp"
            android:layout_marginEnd="3dp"
            app:layout_constraintEnd_toEndOf="parent">

            <TextView
                android:id="@+id/textView15"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginTop="3dp"
                android:text="@string/Observacion"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textSize="12sp"
                android:visibility="invisible"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tvUniMedida" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="3dp"
                android:text="@string/Observacion"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textSize="12sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView9" />

            <TextView
                android:id="@+id/tvObservaciones"
                android:layout_width="254dp"
                android:layout_height="36dp"
                android:layout_marginTop="4dp"
                android:text="@string/DescArt1"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textSize="12sp"
                android:textStyle="bold"
                app:layout_constraintStart_toEndOf="@+id/textView14"
                app:layout_constraintTop_toBottomOf="@+id/textView9" />

            <TextView
                android:id="@+id/textView13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="3dp"
                android:text="@string/UnidadMedida"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textSize="12sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tvDescArt" />

            <TextView
                android:id="@+id/tvUniMedida"
                android:layout_width="225dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="3dp"
                android:layout_marginTop="3dp"
                android:text="@string/UnidadMedida1"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textSize="12sp"
                android:textStyle="bold"
                app:layout_constraintStart_toEndOf="@+id/textView13"
                app:layout_constraintTop_toBottomOf="@+id/tvDescArt" />

            <TextView
                android:id="@+id/textView12"
                android:layout_width="69dp"
                android:layout_height="14dp"
                android:layout_marginTop="3dp"
                android:text="@string/Rubro"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textSize="12sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView6" />

            <TextView
                android:id="@+id/tvtime"
                android:layout_width="286dp"
                android:layout_height="16dp"
                android:layout_marginStart="3dp"
                android:layout_marginTop="3dp"
                android:text="@string/Rubro1"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textSize="12sp"
                android:textStyle="bold"
                app:layout_constraintStart_toEndOf="@+id/textView12"
                app:layout_constraintTop_toBottomOf="@+id/tvTipo" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="67dp"
                android:layout_height="14dp"
                android:layout_marginTop="3dp"
                android:text="@string/Tipo"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textSize="12sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView2" />

            <TextView
                android:id="@+id/tvTipo"
                android:layout_width="293dp"
                android:layout_height="17dp"
                android:layout_marginStart="3dp"
                android:layout_marginTop="3dp"
                android:text="@string/Tipo1"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textSize="12sp"
                android:textStyle="bold"
                app:layout_constraintStart_toEndOf="@+id/textView6"
                app:layout_constraintTop_toBottomOf="@+id/tvCodigo" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="69dp"
                android:layout_height="18dp"
                android:layout_marginTop="3dp"
                android:text="@string/Cant"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textSize="12sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tvUniMedida" />

            <TextView
                android:id="@+id/tvCant"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="3dp"
                android:layout_marginTop="3dp"
                android:text="@string/Cant1"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textSize="12sp"
                android:textStyle="bold"
                app:layout_constraintStart_toEndOf="@+id/textView10"
                app:layout_constraintTop_toBottomOf="@+id/tvUniMedida" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="3dp"
                android:layout_marginTop="3dp"
                android:text="@string/Precio"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textSize="12sp"
                app:layout_constraintStart_toEndOf="@+id/tvCant"
                app:layout_constraintTop_toBottomOf="@+id/tvUniMedida" />

            <TextView
                android:id="@+id/tvCosto"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="3dp"
                android:layout_marginTop="3dp"
                android:text="@string/Precio1"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textSize="12sp"
                android:textStyle="bold"
                app:layout_constraintStart_toEndOf="@+id/textView9"
                app:layout_constraintTop_toBottomOf="@+id/tvUniMedida" />

            <TextView
                android:id="@+id/textView93"
                android:layout_width="32dp"
                android:layout_height="18dp"
                android:layout_marginStart="84dp"
                android:layout_marginTop="4dp"
                android:text="@string/Tot"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textSize="12sp"
                app:layout_constraintStart_toEndOf="@+id/tvCosto"
                app:layout_constraintTop_toBottomOf="@+id/tvUniMedida" />

            <TextView
                android:id="@+id/tvTotal"
                android:layout_width="29dp"
                android:layout_height="17dp"
                android:layout_marginTop="4dp"
                android:text="@string/Precio1"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textSize="12sp"
                android:textStyle="bold"
                app:layout_constraintStart_toEndOf="@+id/textView93"
                app:layout_constraintTop_toBottomOf="@+id/tvUniMedida" />

            <TextView
                android:id="@+id/textView2"
                android:layout_width="67dp"
                android:layout_height="16dp"
                android:text="@string/CodArt"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textSize="12sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="69dp"
                android:layout_height="16dp"
                android:layout_marginTop="3dp"
                android:text="@string/DescArt"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textSize="12sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tvtime" />

            <TextView
                android:id="@+id/tvDescArt"
                android:layout_width="257dp"
                android:layout_height="29dp"
                android:layout_marginStart="3dp"
                android:layout_marginTop="3dp"
                android:text="@string/DescArt1"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textSize="12sp"
                android:textStyle="bold"
                app:layout_constraintStart_toEndOf="@+id/textView11"
                app:layout_constraintTop_toBottomOf="@+id/tvtime" />

            <TextView
                android:id="@+id/tvCodigo"
                android:layout_width="318dp"
                android:layout_height="15dp"
                android:layout_marginStart="3dp"
                android:text="@string/CodArt"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textSize="12sp"
                android:textStyle="bold"
                app:layout_constraintStart_toEndOf="@+id/textView2"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>

</androidx.cardview.widget.CardView>
